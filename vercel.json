{
  "version": 2,
  "builds": [
    {
      "src": "client/dist/public/index.html",
      "use": "@vercel/static-build"
    },
    {
      "src": "server/index.ts",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/server/index.ts"
    },
    {
      "src": "/(.*)",
      "dest": "/client/dist/public/index.html"
    }
  ]
}
